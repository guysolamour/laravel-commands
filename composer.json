{
  "name": "guysolamour/laravel-commands",
  "description": "As suit of useful artisan commands for laravel",
  "license": "MIT",
  "keywords": [
    "command","laravel","artisan"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com",
      "homepage": "https://roland-assale.info",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11",
    "guysolamour/laravel-fsystem": "^1.0",
    "spatie/laravel-db-snapshots": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Command\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Command\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Command\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
